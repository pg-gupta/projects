<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="551.0" prefWidth="528.0"
	style="-fx-background-color: #A1BBC7;"
	xmlns="http://javafx.com/javafx/9.0.4"
	xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="controllers.FXAddMedicineController">
	<children>
		<Label id="addMedicineLabel" layoutX="34.0" layoutY="33.0"
			text="Add Medicine" textFill="#0a3a61" textOverrun="LEADING_ELLIPSIS">
			<font>
				<Font name="System Bold" size="18.0" />
			</font>
		</Label>
		<AnchorPane layoutX="32.0" layoutY="100.0"
			prefHeight="392.0" prefWidth="401.0">
			<children>
				<TextArea fx:id="medicine_name" layoutY="5.0" prefHeight="34.0"
					prefWidth="380.0" promptText="Medicine Name" />
				<TextArea fx:id="drugs" layoutY="48.0" prefHeight="14.0"
					prefWidth="380.0" promptText="Drugs Name" />
				<TextArea fx:id="description" layoutY="89.0" prefHeight="15.0"
					prefWidth="380.0" promptText="Description" />
				<TextArea fx:id="dosage" layoutY="180.0"
					prefHeight="35.0" prefWidth="380.0" promptText="Dosage" />
				<TextArea fx:id="disease_id" layoutY="137.0"
					prefHeight="23.0" prefWidth="380.0" promptText="Disease ID" />
				<JFXButton fx:id="addBtn" layoutY="365.0" prefWidth="98.0"
					styleClass="primary" onAction="#addMedicine" stylesheets="@bootstrap3.css" text="Add" />

			</children>
		</AnchorPane>
	</children>
</AnchorPane>